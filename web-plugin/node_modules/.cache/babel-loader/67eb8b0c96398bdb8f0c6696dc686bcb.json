{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'popupView',\n\n  data() {\n    return {\n      resData: 'loading...',\n      url: '',\n      baseURL: 'http://127.0.0.1:5000/api/model'\n    };\n  },\n\n  methods: {\n    getXSS(q) {\n      return axios({\n        url: this.baseURL + '?q=' + q,\n        method: 'post'\n      });\n    },\n\n    async init() {\n      chrome.storage.local.get(['url'], async function (store) {\n        try {\n          const {\n            data: res\n          } = await axios({\n            url: 'http://127.0.0.1:5000/api/model?q=' + store.url,\n            method: 'post'\n          });\n          this.resData = res;\n          console.log('Get XSS result: ', res);\n        } catch (e) {\n          console.log('Error: ', e);\n        }\n      });\n      alert(this.resData);\n    }\n\n  },\n\n  created() {\n    this.init();\n  }\n\n};","map":{"version":3,"mappings":"AAQA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,YADJ;MAELC,GAAG,EAAE,EAFA;MAGLC,OAAO,EAAE;IAHJ,CAAP;EAKD,CARY;;EASbC,OAAO,EAAE;IACPC,MAAM,CAACC,CAAD,EAAI;MACR,OAAOR,KAAK,CAAC;QACXI,GAAG,EAAE,KAAKC,OAAL,GAAe,KAAf,GAAuBG,CADjB;QAEXC,MAAM,EAAE;MAFG,CAAD,CAAZ;IAID,CANM;;IAOP,MAAMC,IAAN,GAAa;MACXC,MAAM,CAACC,OAAP,CAAeC,KAAf,CAAqBC,GAArB,CAAyB,CAAC,KAAD,CAAzB,EAAkC,gBAAgBC,KAAhB,EAAuB;QACvD,IAAI;UACF,MAAM;YAAEb,IAAI,EAAEc;UAAR,IAAgB,MAAMhB,KAAK,CAAC;YAChCI,GAAG,EAAE,uCAAuCW,KAAK,CAACX,GADlB;YAEhCK,MAAM,EAAE;UAFwB,CAAD,CAAjC;UAIA,KAAKN,OAAL,GAAea,GAAf;UACAC,OAAO,CAACC,GAAR,CAAY,kBAAZ,EAAgCF,GAAhC;QACF,CAPA,CAOE,OAAOG,CAAP,EAAU;UACVF,OAAO,CAACC,GAAR,CAAY,SAAZ,EAAuBC,CAAvB;QACF;MACD,CAXD;MAYAC,KAAK,CAAC,KAAKjB,OAAN,CAAL;IACD;;EArBM,CATI;;EAgCbkB,OAAO,GAAG;IACR,KAAKX,IAAL;EACD;;AAlCY,CAAf","names":["axios","name","data","resData","url","baseURL","methods","getXSS","q","method","init","chrome","storage","local","get","store","res","console","log","e","alert","created"],"sourceRoot":"","sources":["D:\\jaws7\\Desktop\\lab\\code\\web-plugin\\src\\view\\popup.vue"],"sourcesContent":["<template>\n  <div class=\"main_app\">\n    <button @click.prevent=\"init\">Check XSS</button>\n    <div class=\"result\">{{ resData }}</div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'popupView',\n  data() {\n    return {\n      resData: 'loading...',\n      url: '',\n      baseURL: 'http://127.0.0.1:5000/api/model',\n    };\n  },\n  methods: {\n    getXSS(q) {\n      return axios({\n        url: this.baseURL + '?q=' + q,\n        method: 'post',\n      });\n    },\n    async init() {\n      chrome.storage.local.get(['url'], async function (store) {\n        try {\n          const { data: res } = await axios({\n            url: 'http://127.0.0.1:5000/api/model?q=' + store.url,\n            method: 'post',\n          });\n          this.resData = res;\n          console.log('Get XSS result: ', res);\n        } catch (e) {\n          console.log('Error: ', e);\n        }\n      });\n      alert(this.resData);\n    },\n  },\n  created() {\n    this.init();\n  },\n};\n</script>\n\n<style>\n.main_app {\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 40px;\n  width: 200px;\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n}\n.main_app div {\n  font-size: large;\n}\n.main_app button {\n  margin-bottom: 20px;\n}\n.result {\n  width: 100px;\n  height: 100px;\n  border: 1 solid #f00;\n  border-radius: 50%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}