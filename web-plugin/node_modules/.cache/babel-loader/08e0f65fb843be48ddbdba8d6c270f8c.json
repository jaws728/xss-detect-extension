{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'popupView',\n\n  data() {\n    return {\n      resData: 'loading...',\n      url: '',\n      baseURL: 'http://127.0.0.1:5000/api/model'\n    };\n  },\n\n  methods: {\n    async init() {\n      chrome.storage.local.get(['url'], async function (store) {\n        try {\n          const {\n            data: res\n          } = await axios.post('http://127.0.0.1:5000/api/model?q=' + store.url);\n          this.resData = res;\n          console.log('Get XSS result: ', this.resData);\n          alert(store.url);\n          alert(this.resData);\n        } catch (e) {\n          console.log('Error: ', e);\n        }\n      });\n    }\n\n  },\n\n  created() {\n    this.init();\n  }\n\n};","map":{"version":3,"mappings":"AAQA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,WADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,OAAO,EAAE,YADJ;MAELC,GAAG,EAAE,EAFA;MAGLC,OAAO,EAAE;IAHJ,CAAP;EAKD,CARY;;EASbC,OAAO,EAAE;IACP,MAAMC,IAAN,GAAa;MACXC,MAAM,CAACC,OAAP,CAAeC,KAAf,CAAqBC,GAArB,CAAyB,CAAC,KAAD,CAAzB,EAAkC,gBAAgBC,KAAhB,EAAuB;QACvD,IAAI;UACF,MAAM;YAAEV,IAAI,EAAEW;UAAR,IAAgB,MAAMb,KAAK,CAACc,IAAN,CAAW,uCAAuCF,KAAK,CAACR,GAAxD,CAA5B;UACA,KAAKD,OAAL,GAAeU,GAAf;UACAE,OAAO,CAACC,GAAR,CAAY,kBAAZ,EAAgC,KAAKb,OAArC;UACAc,KAAK,CAACL,KAAK,CAACR,GAAP,CAAL;UACAa,KAAK,CAAC,KAAKd,OAAN,CAAL;QACF,CANA,CAME,OAAOe,CAAP,EAAU;UACVH,OAAO,CAACC,GAAR,CAAY,SAAZ,EAAuBE,CAAvB;QACF;MACD,CAVD;IAWD;;EAbM,CATI;;EAwBbC,OAAO,GAAG;IACR,KAAKZ,IAAL;EACD;;AA1BY,CAAf","names":["axios","name","data","resData","url","baseURL","methods","init","chrome","storage","local","get","store","res","post","console","log","alert","e","created"],"sourceRoot":"","sources":["D:\\jaws7\\Desktop\\lab\\code\\web-plugin\\src\\view\\popup.vue"],"sourcesContent":["<template>\n  <div class=\"main_app\">\n    <button @click.prevent=\"init\">Check XSS</button>\n    <div class=\"result\">Loading...</div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default