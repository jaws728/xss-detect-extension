{"ast":null,"code":"import { requestMixin } from \"@/mixins\";\nexport default {\n  name: 'popupView',\n  mixins: [requestMixin],\n\n  data() {\n    return {\n      msg: 'popup',\n      isXss: false,\n      resData: ''\n    };\n  },\n\n  methods: {\n    getXSS() {\n      // const data = api('/api', {url: window.location.pathname})\n      const response = this.getXSS('asn');\n      console.log(response.data);\n      this.resData = response.data; // this.isXss = data != 0 ? true : false\n    }\n\n  },\n\n  mounted() {\n    let bg = chrome.extension.getBackgroundPane();\n    chrome.tabs.query({\n      active: true,\n      currentWindow: true\n    }, tabs => {\n      this.resData = bg.perfWatch[tabs[0].url];\n    }); // this.resData = 'Mounted'\n  }\n\n};","map":{"version":3,"mappings":"AASA,SAAQA,YAAR,QAA2B,UAA3B;AACA,eAAe;EACbC,IAAI,EAAE,WADO;EAEbC,MAAM,EAAE,CAACF,YAAD,CAFK;;EAGbG,IAAG,GAAK;IACN,OAAO;MACLC,GAAG,EAAE,OADA;MAELC,KAAK,EAAE,KAFF;MAGLC,OAAO,EAAE;IAHJ,CAAP;EAKD,CATY;;EAUbC,OAAO,EAAE;IACPC,MAAM,GAAG;MACP;MACA,MAAMC,QAAO,GAAI,KAAKD,MAAL,CAAY,KAAZ,CAAjB;MACAE,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACN,IAArB;MACA,KAAKG,OAAL,GAAeG,QAAQ,CAACN,IAAxB,CAJO,CAKP;IACF;;EAPO,CAVI;;EAmBbS,OAAO,GAAE;IACP,IAAIC,EAAC,GAAIC,MAAM,CAACC,SAAP,CAAiBC,iBAAjB,EAAT;IACAF,MAAM,CAACG,IAAP,CAAYC,KAAZ,CAAkB;MAACC,MAAM,EAAE,IAAT;MAAeC,aAAa,EAAE;IAA9B,CAAlB,EAAwDH,IAAD,IAAU;MAC/D,KAAKX,OAAL,GAAeO,EAAE,CAACQ,SAAH,CAAaJ,IAAI,CAAC,CAAD,CAAJ,CAAQK,GAArB,CAAf;IACD,CAFD,EAFO,CAKP;EACF;;AAzBa,CAAf","names":["requestMixin","name","mixins","data","msg","isXss","resData","methods","getXSS","response","console","log","mounted","bg","chrome","extension","getBackgroundPane","tabs","query","active","currentWindow","perfWatch","url"],"sourceRoot":"","sources":["/Users/p/Desktop/thesis/lab/code/web-plugin/src/view/popup.vue"],"sourcesContent":["<template>\n  <div class=\"main_app\">\n    <h1>{{resData}}</h1>\n    <div v-if=\"isXss\">There is a XSS attack!</div>\n    <div v-else>This site is safe.</div>\n  </div>\n</template>\n\n<script>\nimport {requestMixin} from \"@/mixins\"\nexport default {\n  name: 'popupView',\n  mixins: [requestMixin],\n  data () {\n    return {\n      msg: 'popup',\n      isXss: false,\n      resData: ''\n    }\n  },\n  methods: {\n    getXSS() {\n      // const data = api('/api', {url: window.location.pathname})\n      const response = this.getXSS('asn')\n      console.log(response.data)\n      this.resData = response.data\n      // this.isXss = data != 0 ? true : false\n    }\n  },\n  mounted(){\n    let bg = chrome.extension.getBackgroundPane()\n    chrome.tabs.query({active: true, currentWindow: true}, (tabs) => {\n      this.resData = bg.perfWatch[tabs[0].url]\n    })\n    // this.resData = 'Mounted'\n  }\n}\n\n</script>\n\n<style>\n.main_app {\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n  width: 200px;\n}\n.main_app div {\n  font-size: large;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}